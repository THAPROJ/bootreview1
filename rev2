#!/bin/bah 
echo "enter a number between 10 to 20"

read number
 
if [ $number -gt 20 -a $number -lt 10 ]
then
else
  echo"invalid numbber"
else
for(( i=$number; i<=1; i-- ))
      if [ $number -ne 20 -a $number -ne 15 -a $number -ne 10 -a $number -ne 5 ]
      then
         echo $number
      fi
done
fi
